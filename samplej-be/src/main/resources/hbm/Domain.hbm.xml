<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rms.rms.service.model.Domain" table="domains">
        <id name="id" type="java.lang.String" column="id">
            <generator class="com.rms.rms.persistence.InquisitiveUUIDGenerator"/>
        </id>

        <property name="provinceId" type="java.lang.String" column="province_id"/>
        <property name="code" type="java.lang.String" column="code"/>
        <property name="name" type="java.lang.String" column="name"/>
        <property name="status" type="java.lang.String" column="status"/>
        <property name="type" type="java.lang.String" column="type"/>
    </class>
</hibernate-mapping>